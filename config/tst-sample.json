{
  "environment": "tst",
  "environmentName": "Test",
  "meshName": "meshName-test",
  "domains": [
    {
      "domainName": "SolaceSampleDomain",
      "applications": [
        {
          "name": "Application_1",
          "version": "0.1.0",
          "user": {
            "name": "app1-tst",
            "type": "solaceClientUsername",
            "password": "app1-tst"
          }
        },
        {
          "name": "Application_2",
          "version": "0.1.0",
          "user": {
            "name": "app2-tst",
            "type": "solaceClientCertificate",
            "clientProfileName": "[some predefined existing client profile template name]"
          }
        },
        {
          "name": "Application_3",
          "version": "0.1.0",
          "user": {
            "name": "app3_role",
            "type": "solaceAuthorizationGroup"
          }
        },
        {
          "name": "RDP_Application_1",
          "version": "0.1.0"
        }
      ]
    }
  ],
  "brokers": [ {
    "name": "brokerName",
    "url": "https://mr-connection-[brokerId].messaging.solace.cloud:943/SEMP/v2/config",
    "user": "mission-control-manager",
    "password": "<secret>",
    "msgVpnName": "vpnName"
  }]
}
